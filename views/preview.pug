extend _basepage.pug

block link
  link(rel="stylesheet" href=`/css/themes/${themeId}/theme.css`)
  link(rel="stylesheet" href=`/css/themes/${themeId}/colorpreview.css`)
  link(rel="stylesheet" href="/css/layout.css")

mixin tabpane(id)
  if id === defaultTab
    .tab-pane.active(id=`page-${id}`)
      block
  else
    .tab-pane(id=`page-${id}`)
      block

mixin dropdown-content
  <ul class="dropdown-menu #{attributes.class}">
    <li><a class="dropdown-item" href="#">Action</a></li>
    <li><a class="dropdown-item" href="#">Another action</a></li>
    <li><a class="dropdown-item disabled" href="#">Something else here</a></li>
    <li><hr class="dropdown-divider"></li>
    <li><a class="dropdown-item active" href="#">Separated link</a></li>
  </ul>

mixin table-content
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>Mark</td>
      <td>Otto</td>
      <td>@mdo</td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>Thornton</td>
      <td>@fat</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td colspan="2">Larry the Bird</td>
      <td>@twitter</td>
    </tr>
  </tbody>
  <tfoot>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </tfoot>

mixin navbar-content(variant)
  <div class="container-fluid">
    <a class="navbar-brand" href="#">Navbar</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#preview-navbar-supported-content-#{i}" aria-controls="preview-navbar-supported-content-#{i}" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="preview-navbar-supported-content">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="#">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Link</a>
        </li>
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle" href="#" id="preview-navbar-dropdown-#{i}" role="button" data-bs-toggle="dropdown" aria-expanded="false">
            Dropdown
          </a>
          <ul class="dropdown-menu" aria-labelledby="preview-navbar-dropdown-#{i}">
            <li><a class="dropdown-item" href="#">Action</a></li>
            <li><a class="dropdown-item" href="#">Another action</a></li>
            <li><hr class="dropdown-divider"></li>
            <li><a class="dropdown-item" href="#">Something else here</a></li>
          </ul>
        </li>
        <li class="nav-item">
          <a class="nav-link disabled">Disabled</a>
        </li>
      </ul>
      <form class="d-flex">
        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn #{outlineButtonInBG(variant)}" type="submit">Search</button>
      </form>
    </div>
  </div>

block content

  -
    function whiteTextOnCard(variant) {
      if (theme.applyWhiteTextOnCards && theme.whiteTextVariants.includes(variant) ) {
        return "text-white";
      } else {
        return "";
      }
    }
    function whiteText(variant) {
      if (theme.whiteTextVariants.includes(variant) ) {
        return "text-white";
      } else {
        return "";
      }
    }
    function whiteCloseButton(variant) {
      if (theme.whiteTextVariants.includes(variant) ) {
        return "btn-close-white";
      } else {
        return "";
      }
    }
    function darkText(variant) {
      if (!theme.whiteTextVariants.includes(variant) ) {
        return "text-dark";
      } else {
        return "";
      }
    }
    function navbarScheme(variant) {
      if (theme.whiteTextVariants.includes(variant) ) {
        return "navbar-dark";
      } else {
        return "navbar-light";
      }
    }
    function outlineButtonInBG(variant) {
      if (theme.whiteTextVariants.includes(variant) ) {
        return "btn-outline-light";
      } else {
        return "btn-outline-dark";
      }
    }

  .row
    .col-md-3#preview-sidebar
      ul.nav.nav-pills.flex-column
        each title, id in tabs
          li.nav-item
            if id === defaultTab
              button.nav-link.active(data-bs-toggle="tab" data-bs-target=`#page-${id}`)= title
            else
              button.nav-link(data-bs-toggle="tab" data-bs-target=`#page-${id}`)= title
    
    svg(xmlns="http://www.w3.org/2000/svg" style="display: none;")
      symbol#check-circle-fill(fill="currentColor" viewbox="0 0 16 16")
        path(d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z")
      symbol#info-fill(fill="currentColor" viewbox="0 0 16 16")
        path(d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412-1 4.705c-.07.34.029.533.304.533.194 0 .487-.07.686-.246l-.088.416c-.287.346-.92.598-1.465.598-.703 0-1.002-.422-.808-1.319l.738-3.468c.064-.293.006-.399-.287-.47l-.451-.081.082-.381 2.29-.287zM8 5.5a1 1 0 1 1 0-2 1 1 0 0 1 0 2z")
      symbol#exclamation-triangle-fill(fill="currentColor" viewbox="0 0 16 16")
        path(d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z")
      symbol#sample-image(fill="currentColor" style="font-size:1.125rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#868e96")
        text(x="50%" y="50%" fill="#dee2e6" dy=".3em") Sample image
      symbol#carousel-slide1(fill="currentColor" style="font-size:1.5rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#666")
        text(x="50%" y="50%" fill="#dee2e6" dy=".3em") First slide
      symbol#carousel-slide2(fill="currentColor" style="font-size:1.5rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#555")
        text(x="50%" y="50%" fill="#dee2e6" dy=".3em") Second slide
      symbol#carousel-slide3(fill="currentColor" style="font-size:1.5rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#444")
        text(x="50%" y="50%" fill="#dee2e6" dy=".3em") Third slide
      symbol#carousel-slide1-inv(fill="currentColor" style="font-size:1.5rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#CCC")
        text(x="50%" y="50%" fill="#333" dy=".3em") First slide
      symbol#carousel-slide2-inv(fill="currentColor" style="font-size:1.5rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#DDD")
        text(x="50%" y="50%" fill="#333" dy=".3em") Second slide
      symbol#carousel-slide3-inv(fill="currentColor" style="font-size:1.5rem;text-anchor:middle")
        rect(width="100%" height="100%" fill="#EEE")
        text(x="50%" y="50%" fill="#333" dy=".3em") Third slide

    .col-12.col-md.mt-4.mt-md-0.tab-content

      h1= theme.name
      hr

