      +tabpane("buttons")
        <h2>Buttons</h2>

        <div class="mb-3">
          each color in colorVariants
            <button type="button" class="btn btn-#{color}">#{color}</button>&nbsp;
          <button type="button" class="btn btn-link">Link</button>
        </div>
